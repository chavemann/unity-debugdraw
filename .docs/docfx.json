{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "c.DebugDraw.Runtime.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "DebugDraw Documentation",
      "_appFooter": "DebugDraw Documentation",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "sitemap":
    {
      "baseUrl": "https://normanderwan.github.io/DocFxForUnity"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/modern-mod"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xref1": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "xrefService1": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}
